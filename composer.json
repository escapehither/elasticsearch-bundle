{
    "name": "escapehither/search-manager-bundle",
    "description": " This symfony bundle provide search and index features for symfony application",
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "3.*",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "symfony/polyfill-apcu": "^1.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "stof/doctrine-extensions-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "guzzlehttp/guzzle": "^6.2",
        "symfony/serializer": "^3.2",
        "white-october/pagerfanta-bundle": "^1.0",
        "elasticsearch/elasticsearch": "^5.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "behat/symfony2-extension": "^2.1",
        "behat/behat": "^3.3",
        "phpunit/phpunit": "^4.8.35 || ^5.7.11 || ^6.5",
        "phpunit/php-invoker":"~1.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-zombie-driver": "^1.4",
        "behat/mink-extension": "^2.2",
        "behat/web-api-extension": "~1.0@dev",
        "escapestudios/symfony2-coding-standard": "~2.0"
    },
    "autoload": {
        "psr-4": { "EscapeHither\\SearchManagerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "authors": [
        {
            "name": "shorinmaru",
            "email": "sho_fukamachi@hotmail.fr"
        }
    ]
}
